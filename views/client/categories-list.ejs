<% include ./partials/head.ejs %>
<% include ./partials/header.ejs %>
<% include ./partials/navigation.ejs %>
<main class="main">
        <h1 class="main-title"><%= title %></h1>
            <% if(products.length == 0) { %>
                <p>Der er ingen produkter at vise</p>
            <% } %>
                <a href="/produkter/0">Tilbage til kategorier</a>
            <% products.forEach(product => { %>
                <article class="product">
                    <a href="/produkt/<%= product.productid %>">
                    <img class="list-product-image" src="images/uploads/<%= product.image %>" alt="">
                    <%= product.name %>
                    </a>
                </article>
            <% }); %>
    <div class="pagination">
        <a href="" class="btn">Forrige side</a>
        <a href="" class="btn">NÃ¦ste side</a>
    </div>
</main>
